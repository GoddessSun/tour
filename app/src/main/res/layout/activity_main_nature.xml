            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen48"
            android:background="@color/white"
            android:paddingLeft="@dimen/dimen20"
            android:paddingRight="@dimen/dimen20">
            <TextView
                android:layout_width="wrap_content"
