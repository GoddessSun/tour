            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen48"
            android:background="@color/white"
            android:paddingLeft="@dimen/dimen20"
            android:paddingRight="@dimen/dimen20">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/title_small_txt_size"
                android:textColor="@color/text_color333"
                android:text="适用人数"
                android:layout_centerVertical="true"/>
            <TextView
                android:id="@+id/store_edit_nature_apply_person_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textSize="@dimen/title_small_txt_size"
                android:textColor="@color/blue43"
                android:text="选择"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:paddingLeft="@dimen/dimen20"/>
