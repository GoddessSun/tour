            android:paddingLeft="@dimen/dimen20"
            android:paddingRight="@dimen/dimen20">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/title_small_txt_size"
                android:textColor="@color/text_color333"
                android:text="位置"
                android:layout_centerVertical="true"/>
            <TextView
                android:id="@+id/store_edit_nature_location_tv"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textSize="@dimen/title_small_txt_size"
                android:textColor="@color/blue43"
                android:text="选择"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:paddingLeft="@dimen/dimen20"/>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/base_line_height"
            android:background="@color/grayf2"
            android:layout_marginLeft="@dimen/dimen20"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen48"
            android:background="@color/white"
            android:paddingLeft="@dimen/dimen20"
            android:paddingRight="@dimen/dimen20">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/title_small_txt_size"
                android:textColor="@color/text_color333"
                android:text="补给类型"
                android:layout_centerVertical="true"/>
